<!DOCTYPE html>
<html>
<head lang="en">
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>{{ config.APP_NAME }}</title>

  {# https://realfavicongenerator.net/ #}
  <link rel="apple-touch-icon" sizes="180x180" href="{{ static_url_for('favicon/apple-touch-icon.png') }}">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ static_url_for('favicon/favicon-32x32.png') }}">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ static_url_for('favicon/favicon-16x16.png') }}">
  <link rel="manifest" href="{{ static_url_for('favicon/site.webmanifest') }}">
  <link rel="mask-icon" href="{{ static_url_for('favicon/safari-pinned-tab.svg') }}" color="#000000">
  <link rel="shortcut icon" href="{{ static_url_for('favicon/favicon.ico') }}">
  <meta name="msapplication-TileColor" content="#000000">
  <meta name="msapplication-config" content="{{ static_url_for('favicon/browserconfig.xml') }}">
  <meta name="theme-color" content="#ffffff">

  <script src="https://kit.fontawesome.com/27892f27e9.js" crossorigin="anonymous"></script>
  <link
    rel="stylesheet"
    href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
    integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk"
    crossorigin="anonymous"
  />

  <script type="text/javascript" src="{{ static_url_for('js/shared.bundle.js') }}"></script>

  {% block head %}{% endblock %}
</head>
<body>
<div id="content"></div>

{% block body %}{% endblock %}
</body>
</html>
